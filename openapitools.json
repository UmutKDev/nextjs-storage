{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.17.0",
    "generators": {
      "test": {
        "generatorName": "typescript-axios",
        "inputSpec": "http://localhost:8080/swagger-json",
        "output": "Service/Generates",
        "skipValidateSpec": true,
        "globalProperty": {
          "skipFormModel": false,
          "withInterfaces": true
        },
        "additionalProperties": {
          "useSingleRequestParameter": true,
          "supportsES6": true,
          "modelPackage": "dto"
        }
      }
    }
  }
}
